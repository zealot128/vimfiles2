########################################
# JS snippets - for Rails, see below #
########################################

snippet fun
	function(${1}) {
		${2}
	}
snippet fun)
	function(${1}) {
		${2}
	});

snippet if
	if (${1}) {
		${2}
	}
snippet ife
	if (${1}) {
		${2}
	} else {
		${3}
	}
snippet $
	$("${1}").${2}
snippet $t
	$(this).${1}

snippet cl
	console.log(${1});
